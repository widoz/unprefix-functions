{
  "name": "widoz/wp-functions",
  "description": "WordPress functions utility",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "wordpress",
    "library"
  ],
  "authors": [
    {
      "name": "Guido Scialfa",
      "email": "dev@guidoscialfa.com"
    }
  ],
  "minimum-stability": "dev",
  "support": {
    "issues": "https://github.com/widoz/wp-functions"
  },
  "require": {
    "php": ">=7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "brain/monkey": "^2.0",
    "widoz/project-tests-helper": "dev-phpunit-8",
    "inpsyde/php-coding-standards": "^0.13",
    "fzaninotto/faker": "^1.9@dev",
    "johnpbloch/wordpress-core": "dev-master",
    "vimeo/psalm": "^3.0@dev"
  },
  "autoload": {
    "psr-4": {
      "Widoz\\Wp\\Functions\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Widoz\\Wp\\Functions\\Tests\\": "tests/php",
      "Widoz\\Wp\\Functions\\Tests\\Unit\\": "tests/php/Unit"
    }
  },
  "extra": {
    "installer-disable": true,
    "branch-alias": {
      "dev-develop": "0.1.x-dev"
    }
  },
  "config": {
    "platform": {
      "php": "7.3.16"
    }
  }
}
